{
"extends": [
    "prettier",
    "prettier/react",
  ],
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
        "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
},
  "env": {
    "browser": true,
    "node": true
  },

  "rules": {
    "quotes": ["warn", "single"],
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
    "strict": ["error", "never"],
    "no-console": ["warn"],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
  }

}
